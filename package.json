{
  "name": "tsync",
  "version": "0.1.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com:rodogir/tsync.git"
  },
  "author": "rodogir <me@rodogir.com>",
  "license": "MIT",
  "engines": {
    "node": "6.10.2",
    "npm": "3.10.10"
  },
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "npm run build:server && concurrently \"npm run build:server -- -w\" \"nodemon dist/server/index.js\"",
    "build": "npm run clean && concurrently \"npm run build:server\" \"npm run build:client\"",
    "build:server": "tsc -p ./tsconfig.server.json",
    "build:client": "webpack",
    "heroku-postbuild": "npm run build",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@types/dotenv": "4.0.0",
    "@types/express": "4.0.35",
    "@types/node": "as",
    "@types/react": "15.0.23",
    "@types/react-dom": "15.5.0",
    "@types/winston": "2.3.1",
    "awesome-typescript-loader": "3.1.3",
    "concurrently": "3.4.0",
    "dotenv": "4.0.0",
    "express": "4.15.2",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "source-map-loader": "0.2.1",
    "typescript": "2.3.2",
    "webpack": "2.4.1",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-plugin-import": "2.2.0",
    "nodemon": "1.11.0",
    "tslint": "5.1.0"
  }
}
