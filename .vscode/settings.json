{
  "tslint.enable": true,
  "tslint.autoFixOnSave": true,
  "autoimport.doubleQuotes": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "dist": true,
    ".awcache": true
  },
  "files.watcherExclude": {
